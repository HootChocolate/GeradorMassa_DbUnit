package br.ce.wcaquino.service;

import br.ce.wcaquino.dao.impl.MassaDAOImpl;

public class MassaService {

	private static MassaDAOImpl dao;
	
	public MassaService() {
		dao = new MassaDAOImpl();
	}
	
	/**Chama um método na classe MassaDAOImpl de inserção de dados, que insere uma conta no banco.
	 * 
	 * @throws Exception
	 */
	public static void inserirDadoNaContaDoSeuBarriga() throws Exception {
		dao.inserirConta();
	}
	
	public static void main(String[] args) throws Exception {
		inserirDadoNaContaDoSeuBarriga();
	}
}

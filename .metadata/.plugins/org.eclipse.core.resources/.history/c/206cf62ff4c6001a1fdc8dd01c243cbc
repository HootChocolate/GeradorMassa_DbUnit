package br.ce.wcaquino.entidades;

public class ContaSeuBarriga {

	private long id;
	private String contaSeuBarriga;
	
	public ContaSeuBarriga() {}
	
	public void setId(long id) {
		this.id = id;
	}
	
	public long getId() {
		return id;
	}
	
	public void setContaSeuBarriga(String contaSeuBarriga) {
		this.contaSeuBarriga = contaSeuBarriga;
	}
	
	public String getContaSeuBarriga() {
		return contaSeuBarriga;
	}
	
	@Override
	public String toString() {
		System.out.println("==========================");
		System.out.println("      Dados da Conta       \n");
		System.out.println("id: " + getId());
		System.out.println("Nome: " + getContaSeuBarriga());
		System.out.println("==========================");
		return print;
	}
}
